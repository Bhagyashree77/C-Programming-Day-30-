#include<stdio.h>
#include<conio.h>
main() 
{
    int n,i,sum=0;
    printf("Enter the number of elements in the array: ");
    scanf("%d",&n);
    int a[n];
    printf("Enter elements in the array:\n");
    for (i=0;i<n;i++) 
    {
        scanf("%d",&a[i]);
        sum=sum+a[i];
    }
    printf("The sum of elements in the array is %d",sum);
}
