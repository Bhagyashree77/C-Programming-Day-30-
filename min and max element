#include<stdio.h>
#include<conio.h>
main()
{
    int a[10],n,i,max,min;
    printf("enter the size of array\n");
    scanf("%d",&n);
    printf("enter elements into array\n");
    for(i=0;i<=n-1;i++)
    {
        scanf("%d",&a[i]);
    }
    max=a[0];
    for(i=1;i<=n-1;i++)
    {
        if(a[i]>max)
        {
            max=a[i];
        }
    }
    printf("The max element is %d\n",max);
    min=a[0];
    for(i=1;i<=n-1;i++)
    {
        if(a[i]<min)
        {
            min=a[i];
        }
    }
    printf("The min element is %d\n",min);
}
